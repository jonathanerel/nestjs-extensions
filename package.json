{
  "name": "bp-npmnestjs-extensions",
  "version": "0.0.0-alpha0",
  "description": "Nestjs lib extensions",
  "main": "index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier lib/**/*.ts --write",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathanerel/nestjs-extensions.git"
  },
  "keywords": [
    "nestjs",
    "nest",
    "bp",
    "service",
    "microservices",
    "extensions"
  ],
  "author": "Jonathan Erel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonathanerel/nestjs-extensions/issues"
  },
  "homepage": "https://github.com/jonathanerel/nestjs-extensions#readme",
  "devDependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@types/node": "^10.3.5",
    "lint-staged": "^7.2.2",
    "prettier": "^1.11.1",
    "typescript": "^2.6.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0"
  },
  "optionalDependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add -f"
    ]
  }
}
